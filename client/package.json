{
  "name": "client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --open",
    "build": "ng build",
    "deploy": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --config ../.prettierrc.yaml --ignore-path ../.prettierignore --write \"src/**/*\"",
    "test": "ng test",
    "coverage": "ng test --code-coverage --source-map=true",
    "lint": "ng lint --format stylish",
    "lint:fix": "npm run lint -- --fix",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --base-href ./ && electron-packager . --asar --ignore=/node_modules --ignore=/.angular --ignore=/e2e --ignore=/src --overwrite --out=./build --electron-version=28.2.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "15.2.10",
    "@angular/compiler": "15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "15.2.10",
    "@angular/platform-browser-dynamic": "15.2.10",
    "@angular/router": "15.2.10",
    "angular": "^1.8.3",
    "bootstrap": "^5.2.3",
    "rxjs": "^7.5.6",
    "socket.io-client": "4.5.1",
    "tslib": "2.4.0",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.10",
    "@angular/compiler-cli": "15.2.10",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^18.19.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "electron": "^28.2.7",
    "electron-packager": "^17.1.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "43.0.2",
    "jasmine-core": "4.3.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "2.7.1",
    "typescript": "4.9.5"
  }
}
